                                <Font name="Encode Sans Light" size="15.0"/>
                            </font>
                            <cursor>
                                <Cursor fx:constant="HAND"/>
                            </cursor>
                        </Button>
                        <Button layoutX="612.0" layoutY="5.0" mnemonicParsing="false"
                                style="-fx-background-color: #14938D;" text="Ayuda" textFill="WHITE">
                            <cursor>
                                <Cursor fx:constant="HAND"/>
                            </cursor>
                        </Button>
                    </children>
                </Pane>
                <Button fx:id="closeButton" layoutX="889.0" layoutY="2.0" maxHeight="-Infinity" maxWidth="-Infinity"
                        minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#CloseWindow"
                        prefHeight="24.0" prefWidth="24.0"
                        style="-fx-content-display: center, center; -fx-background-size: cover, auto; -fx-background-color: #00A39F;"/>
                <Button fx:id="maximizeButton" layoutX="865.0" layoutY="2.0" maxHeight="-Infinity" maxWidth="-Infinity"
                        minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#MaximizeWindow"
                        prefHeight="24.0" prefWidth="24.0"
                        style="-fx-content-display: center , center; -fx-background-size: cover, auto; -fx-background-color: #00A39F;"/>
                <Button fx:id="minimizeButton" layoutX="841.0" layoutY="2.0" maxHeight="-Infinity" maxWidth="-Infinity"
                        minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#MinimizeWindow"
                        prefHeight="24.0" prefWidth="24.0"
                        style="-fx-content-display: center, center; -fx-background-size: cover, auto; -fx-background-color: #00A39F;"/>
