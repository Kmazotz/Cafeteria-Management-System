                        <Button layoutX="81.0" layoutY="4.0" mnemonicParsing="false"
                                style="-fx-background-color: #14938D;" text="Inventario" textFill="WHITE">
                            <cursor>
                                <Cursor fx:constant="HAND"/>
                            </cursor>
                        </Button>
                        <Button layoutX="200.0" layoutY="4.0" mnemonicParsing="false"
                                style="-fx-background-color: #14938D;" text="Facturación" textFill="WHITE">
                            <font>
                                <Font name="Encode Sans Light" size="15.0"/>
                            </font>
                            <cursor>
                                <Cursor fx:constant="HAND"/>
                            </cursor>
                        </Button>
                        <Button layoutX="325.0" layoutY="4.0" mnemonicParsing="false"
                                style="-fx-background-color: #14938D;" text="Gestion de Usuarios" textFill="WHITE">
                            <cursor>
                                <Cursor fx:constant="HAND"/>
                            </cursor>
                        </Button>
                        <Button layoutX="509.0" layoutY="4.0" mnemonicParsing="false"
