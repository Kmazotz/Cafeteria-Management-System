    <children>
        <Pane layoutX="-1.0" prefHeight="153.0" prefWidth="915.0" style="-fx-background-color: #00A39F;">
            <children>
                <Pane layoutY="114.0" prefHeight="39.0" prefWidth="915.0" style="-fx-background-color: #14938D;">
